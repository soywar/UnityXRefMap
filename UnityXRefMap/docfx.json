{
  "metadata": [
    {
      "src": [
        {
          "files": [ "UnityCsReference/Projects/CSharp/*.csproj" ]
        }
      ],
      "filter": "filterConfig.yml",
      "dest": "ScriptReference"
    }
  ],
  "build":
  {
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "content": [
      {
        "files": [ "ScriptReference/**.yml" ]
      }
    ],
    "dest": "build"
  }
}